{
  "name": "tonx86",
  "version": "0.2.0",
  "description": "Educational x86-like assembly environment for VS Code",
  "private": true,
  "author": {
    "name": "Anthony (Toniboy1)",
    "url": "https://github.com/Toniboy1"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Toniboy1/TonX86.git"
  },
  "bugs": {
    "url": "https://github.com/Toniboy1/TonX86/issues"
  },
  "homepage": "https://github.com/Toniboy1/TonX86#readme",
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "vscode": "^1.8.0"
  },
  "scripts": {
    "build": "npm run build -w packages/simcore && npm -ws run build",
    "compile": "npm -ws run compile",
    "watch": "npm -ws run watch",
    "dev": "npm run watch",
    "test": "npm -ws run test",
    "lint": "npm -ws run lint",
    "check": "npm run lint && npm run build && npm test",
    "precommit": "npm run check",
    "package": "npm run build && cd packages/extension && npm run package",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:patch": "standard-version --release-as patch",
    "release:first": "standard-version --first-release"
  },
  "devDependencies": {
    "@vscode/vsce": "^3.7.1",
    "esbuild": "^0.27.3",
    "npm-run-all": "^4.1.5",
    "standard-version": "^9.5.0"
  }
}
