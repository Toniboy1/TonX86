{
  "comments": {
    "lineComment": ";"
  },
  "brackets": [
    ["[", "]"],
    ["(", ")"],
    ["{", "}"]
  ],
  "autoClosingPairs": [
    ["[", "]"],
    ["(", ")"],
    ["{", "}"],
    ["\"", "\""],
    ["'", "'"]
  ],
  "surroundingPairs": [
    ["[", "]"],
    ["(", ")"],
    ["{", "}"],
    ["\"", "\""],
    ["'", "'"]
  ],
  "folding": {
    "offSide": true
  },
  "wordPattern": "(-?\\d*\\.\\d\\w*)|([^\\`\\~\\!\\@\\#\\%\\^\\&\\*\\(\\)\\-\\=\\+\\[\\{\\]\\}\\\\\\|\\;\\:\\'\\\"\\,\\.\\<\\>\\/\\?\\s]+)",
  "indentationRules": {
    "increaseIndentPattern": "^\\s*[a-zA-Z_][a-zA-Z0-9_]*:\\s*$",
    "decreaseIndentPattern": "^\\s*$"
  }
}
