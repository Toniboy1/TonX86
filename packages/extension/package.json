{
  "name": "tonx86",
  "displayName": "TonX86",
  "description": "Educational x86-like assembly environment for VS Code",
  "version": "0.0.1",
  "publisher": "tonx86",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Debuggers",
    "Programming Languages",
    "Education"
  ],
  "activationEvents": ["*"],
  "main": "out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "tonx86.run",
        "title": "TonX86: Run",
        "category": "TonX86"
      },
      {
        "command": "tonx86.pause",
        "title": "TonX86: Pause",
        "category": "TonX86"
      },
      {
        "command": "tonx86.stepOver",
        "title": "TonX86: Step Over",
        "category": "TonX86"
      },
      {
        "command": "tonx86.stepIn",
        "title": "TonX86: Step In",
        "category": "TonX86"
      },
      {
        "command": "tonx86.stepOut",
        "title": "TonX86: Step Out",
        "category": "TonX86"
      },
      {
        "command": "tonx86.reset",
        "title": "TonX86: Reset",
        "category": "TonX86"
      }
    ],
    "breakpoints": [
      {
        "language": "asm"
      }
    ]
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint src",
    "test": "echo no tests"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.84.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
